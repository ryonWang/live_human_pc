{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "AI数字人系统获客宝",
  "version": "0.1.1",
  "identifier": "cn.1mx.a",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173/",
    "beforeDevCommand": "npm run before:dev",
    "beforeBuildCommand": "npm run before:build"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "label": "main",
        "title": "AI数字人系统获客宝",
        "width": 1366,
        "height": 768,
        "minHeight": 768,
        "minWidth": 1366,
        "resizable": true,
        "fullscreen": false,
        "decorations": false,
        "theme": "Dark"
      }
    ],
    "security": {
      "csp": null,
      "assetProtocol": {
        "enable": true,
        "scope": ["**"]
      }
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.ico",
      "icons/icon.png"
    ],
    "windows": {
      "wix": {
        "language": "zh-CN"
      }
    }
  }
}
